{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "quotemark": [
        "single"
      ],
      "no-empty": false,
      "interface-name": [
        "never-prefix"
      ],
      "no-empty-interface": false,
      "no-unused-expression": false,
      "object-literal-sort-keys": false,
      "max-classes-per-file": [
        false
      ]
    },
    "rulesDirectory": []
}
